echo "Enter Number1:"
read num1
echo "Enter Number2:"
read num2
echo "Enter Number3:"
read num3

if [ $num1 -gt $num2 ] && [ $num1 -gt $num3 ]
then
    echo "Greater Number is:" $num1
elif [ $num2 -gt $num1 ] && [ $num2 -gt $num3 ]
then
    echo "Greater Number is:" $num2
else
    echo "Greater Number is:" $num3
fi
