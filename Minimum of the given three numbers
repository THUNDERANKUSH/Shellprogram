echo "Enter Number1:"
read num1
echo "Enter Number2:"
read num2
echo "Enter Number3:"
read num3

if [ $num1 -lt $num2 ] && [ $num1 -lt $num3 ]
then
    echo "Minimum Number is:" $num1
elif [ $num2 -lt $num1 ] && [ $num2 -lt $num3 ]
then
    echo "Minimum Number is:"  $num2
else
    echo "Minimum Number is:" $num3
fi
